{
  "scripts": {
    "build": "npm run build:localhost",
    "build:dev": "npm run clean:dist && NODE_ENV=dev npm run webpack --profile",
    "build:localhost": "npm run clean:dist && NODE_ENV=localhost npm run webpack --progress --profile",
    "build:feature": "npm run clean:dist && NODE_ENV=dev npm run webpack --profile",
    "build:prod": "npm run clean:dist && NODE_ENV=production npm run webpack --bail",
    "build:int": "npm run clean:dist && NODE_ENV=production npm run webpack --bail",
    "build:uat": "npm run clean:dist && NODE_ENV=production npm run webpack --bail",
    "build:sit": "npm run clean:dist && NODE_ENV=production npm run webpack --bail",
    "check-coverage": "rm -rf ./coverage && npm run test",
    "clean:dist": "npm run rimraf -- dist",
    "e2e": "npm run protractor config/protractor.conf.js",
    "lint": "tslint -p tsconfig.json -e '**/node_modules/**' -e '**/uxd/**'",
    "precommit": "npm run lint",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev": "http-server dist -c-1 --cors",
    "server:localhost": "npm run webpack-dev-server -- --config config/webpack.local.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:local",
    "start": "npm run server:localhost",
    "test": "npm run lint && karma start --browsers=Chrome",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:localhost": "npm run build:localhost -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run --debug",
    "webdriver-manager": "node ./node_modules/protractor/bin/webdriver-manager",
    "webdriver:start": "node ./node_modules/protractor/bin/webdriver-manager start",
    "webdriver:update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
