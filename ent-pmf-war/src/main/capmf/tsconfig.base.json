{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noEmitHelpers": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jasmine",
      "es6-shim",
      "jquery",
      "jqueryui",
      "select2"
    ]
  }
}
