{
  "rules": {
    "arrow-parens": true,
    "class-name": true,
    "comment-format": [
      false
    ],
    "curly": true,
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "import-blacklist": [
      true,
      "rxjs"
    ],
    "import-spacing": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "log"
    ],
    "no-construct": true,
    "no-duplicate-variable": true,
    "no-debugger": true,
    "no-eval": true,
    "no-empty": true,
    "no-empty-interface": true,
    "no-trailing-whitespace": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "trailing-comma": [
      true, {
        "singleline": "never"
      }
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
